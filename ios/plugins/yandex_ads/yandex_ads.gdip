[config]

name="YandexAds"
binary="yandex_ads.a"

initialization="yandex_ads_init"
deinitialization="yandex_ads_deinit"

[dependencies]

; Use local XCFramework instead of CocoaPods
embedded=["YandexMobileAds.xcframework"]
system=["UIKit.framework", "Foundation.framework", "CoreGraphics.framework", "AdSupport.framework", "AppTrackingTransparency.framework"]
linker_flags=["-ObjC"]

[plist]

NSUserTrackingUsageDescription="This app uses advertising ID for personalized ads"
